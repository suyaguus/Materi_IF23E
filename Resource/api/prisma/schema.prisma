generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

// buat pilihan data satuan
enum Satuan {
  Unit
  Pcs
  Kg
}

model tb_barang {
  id    Int    @id @default(autoincrement())
  kode  String @db.VarChar(15)
  nama  String @db.VarChar(50)
  harga Int
  satuan Satuan
}
